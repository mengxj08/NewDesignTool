<UserControl x:Class="NewDesignTool.Pages.DetermineVariables"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="750" d:DesignWidth="750">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab SelectedSource="/Pages/IDV.xaml" Layout="List">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Independent Variables" Source="/Pages/IDV.xaml" />
                <mui:Link DisplayName="Dependent Variables" Source="/Pages/DV.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>

    </Grid>
</UserControl>
